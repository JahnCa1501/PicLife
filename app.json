{
  "expo": {
    "name": "PicLife",
    "slug": "PicLife",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/components/images/PicLifeIcon.png",
    "splash": {
      "image": "./src/components/images/PicLife.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/components/images/PicLife.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./src/components/images/PicLife.png"
    }
  }
}
